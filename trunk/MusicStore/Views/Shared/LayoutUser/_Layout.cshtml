<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/StyleUser/Css/Site.css")" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/jquery-1.4.4.min.js" type="text/javascript"></script>
</head>
<body>
    <!--BEGIN:ALL-->
    <div id="all">
        <!--BEGIN:BACK-->
        <div id="back">
            <!--BEGIN:HEADER-->
            <div id="header-outer">
                <div id="header">
                    <div id="logo">
                        <object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,29,0"
                            width="675" height="150">
                            <param name="wmode" value="transparent" />
                            <param name="movie" value="Content/StyleUser/Images/3.swf" />
                            <param name="quality" value="high" />
                            <embed src="Content/StyleUser/Images/3.swf" wmode="transparent" quality="high"
                                pluginspage="http://www.macromedia.com/shockwave/download/index.cgi?P1_Prod_Version=ShockwaveFlash"
                                type="application/x-shockwave-flash" width="675" height="150">
                            </embed>
                        </object>
                    </div>
                    <div id="banner">
                    </div>
                </div>
            </div>
            <!--END:HEADER-->
            <!--BEGIN:CONTENTAREA-->
            <div id="contentarea">
                <div id="menu-top">
                </div>
                <!--BEGIN:LEFT-->
                <div id="left">
                    <!--BEGIN:MODLEFT-->
                    <div id="mod-left">
                        <div id="m-l-title">
                            Nhạc hot tháng 3</div>
                        <div id="m-l-content">
                            <p>
                                Bài 001</p>
                        </div>
                    </div>
                    <!--BEGIN:MODLEFT-->
                    <div id="mod-left">
                        <div id="m-l-title">
                            Ca sĩ Việt Nam</div>
                        <div id="m-l-content">
                            @{Html.RenderAction("List", "CaSi");}
                        </div>
                    </div>
                    <!--END:MODLEFT-->
                </div>
                <!--END:LEFT-->
                <!--BEGIN:WRAPPER-->
                <div id="wrapper">
                    <div id="main">
                        <div id="m-c-title">
                            NHẠC CỦA TAO CỦA TAO
                        </div>
                        <div id="m-c-content">
                            @RenderBody()
                        </div>
                    </div>
                </div>
                <!--END:WRAPPER-->
                <!--BEGIN:RIGHT-->
                <div id="right">
                    <div id="mod-right">
                        <div id="m-r-title">
                        </div>
                        <div id="m-r-content">
                        </div>
                    </div>
                </div>
                <!--END:RIGHT-->
            </div>
            <!--END:CONTENTAREA-->
            <!--BEGIN:FOOTER-OUTER-->
            <div id="footer-outer">
                <div id="footer-sub">
                    <div id="footer">
                        Bản quyền thuộc về HPT - Copyright 2011</div>
                </div>
            </div>
            <!--END:FOOTER-OUTER-->
        </div>
        <!--END:BACK-->
    </div>
    <!--END:ALL-->
</body>
</html>
