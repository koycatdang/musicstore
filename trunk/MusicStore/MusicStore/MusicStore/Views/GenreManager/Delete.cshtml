@model MusicStore.Models.THELOAI

@{
    ViewBag.Title = "Xóa thể loại | " + @Model.TenTheLoai;
    Layout = "~/Views/Shared/LayoutAdmin/_Layout.cshtml";
}

<h2>Xóa thể loại</h2>
<h3>
    Bạn có chắc muốn xóa thể loại với tên là @Model.TenTheLoai?</h3>
@using (Html.BeginForm())
{
    <p>
        <input type="submit" value="Xóa" />
    </p>
    <p>
        @Html.ActionLink("Quay lại", "Index")
    </p>
}
