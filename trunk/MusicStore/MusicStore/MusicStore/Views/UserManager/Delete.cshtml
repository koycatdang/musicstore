@model MusicStore.Models.NGUOIDUNG
@{
    ViewBag.Title = "Xóa người dùng | " + @Model.TenNguoiDung;
    Layout = "~/Views/Shared/LayoutAdmin/_Layout.cshtml";
}
<h2>
    Xóa người dùng</h2>
<h3>
    Bạn có chắc muốn xóa <em>@Model.TenNguoiDung</em> cùng với tất cả thông tin liên quan tới
    người dùng này?</h3>
@using (Html.BeginForm())
{
    <p>
        <input type="submit" value="Xóa" />
    </p>
    <p class="goback">
        @Html.ActionLink("<<Quay lại", "Index")
    </p>
}
