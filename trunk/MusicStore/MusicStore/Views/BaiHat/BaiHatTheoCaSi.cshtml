@model IEnumerable<MusicStore.Models.BAIHAT>
@{
    ViewBag.Title = "Bài hát của ca sĩ";
    Layout = "~/Views/Shared/LayoutUser/_Layout.cshtml";
}
<h2>
    Danh sách bài hát</h2>
<ul>
    @foreach (var bh in Model)
    {
        <li><a href="@Url.Action("../BaiHat/Detail", new { id = bh.MaBaiHat })">@bh.TenBaiHat</a><b style="text-align: right">@bh.SoLuongNghe</b>
            @bh.CASI.TenCaSi ---- @bh.NHACSI.TenNhacSi</li>
    }
</ul>
